{
  "name": "ReactCounter",
  "version": "0.1.0",
  "homepage": "http://react-counter.fastwombat.com",
  "private": true,
  "devDependencies": {
    "custom-react-scripts": "0.0.23"
  },
  "dependencies": {
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "mobx-react-router": "^3.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
